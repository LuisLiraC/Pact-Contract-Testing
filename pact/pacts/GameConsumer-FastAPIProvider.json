{
  "consumer": {
    "name": "GameConsumer"
  },
  "interactions": [
    {
      "description": "A request to receive all games",
      "request": {
        "method": "GET",
        "path": "/games/"
      },
      "response": {
        "body": {
          "data": [
            {
              "id": 1,
              "is_released": true,
              "name": "Game 1",
              "primary_color": "ff0000",
              "thumbnail": "http://localhost/static/image.png",
              "year": 2021
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.data": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.data[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].is_released": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].primary_color": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^[0-9a-fA-F]+$"
                }
              ]
            },
            "$.data[*].thumbnail": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "http://localhost/static/.*.(png|jpg|jpeg)"
                }
              ]
            },
            "$.data[*].year": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.4.0"
    },
    "pactRust": {
      "ffi": "0.4.16",
      "models": "1.1.19"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "FastAPIProvider"
  }
}