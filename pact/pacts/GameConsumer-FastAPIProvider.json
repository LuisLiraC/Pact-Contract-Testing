          "header": {
            "Authorization": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "method": "GET",
        "path": "/games/1/"
      },
      "response": {
        "body": {
          "data": {
            "id": 1,
            "is_released": true,
            "name": "Game 1",
            "primary_color": "ff0000",
            "thumbnail": "http://localhost/static/image.png",
            "year": 2021
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
