{
  "consumer": {
    "name": "FavoritesConsumer"
  },
  "interactions": [
    {
      "description": "A request to get all favorite games",
      "providerStates": [
        {
          "name": "An existing user 2"
        },
        {
          "name": "With already added games",
          "params": {
            "game_id": 6
          }
        }
      ],
      "request": {
        "generators": {
          "header": {
            "Authorization": {
              "expression": "Bearer ${token}",
              "type": "ProviderState"
            }
          }
        },
        "headers": {
          "Authorization": "Bearer token"
        },
        "matchingRules": {
          "header": {
            "Authorization": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "method": "GET",
        "path": "/favorites/"
      },
      "response": {
        "body": {
          "data": [
            {
              "game": {
                "id": 1,
                "is_released": true,
                "name": "Game 1",
                "primary_color": "ff0000",
                "thumbnail": "http://localhost/static/image.png",
                "year": 2021
              },
              "id": 13
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.data": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.data[*].game.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].game.is_released": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].game.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].game.primary_color": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^[0-9a-fA-F]+$"
                }
              ]
            },
            "$.data[*].game.thumbnail": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "http://localhost/static/.*.(png|jpg|jpeg)"
                }
              ]
            },
            "$.data[*].game.year": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.4.0"
    },
    "pactRust": {
      "ffi": "0.4.16",
      "models": "1.1.19"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "FastAPIProvider"
  }
}